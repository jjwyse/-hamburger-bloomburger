{
  "name": "bloomburger",
  "version": "0.0.1",
  "description": "Delivers blogs and news, right to your front door",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean-and-copy && NODE_ENV=development webpack --progress --env.dev",
    "build:prod": "npm run clean-and-copy && NODE_ENV=production webpack --progress --env.prod",
    "start": "npm run clean-and-copy && NODE_ENV=development babel-node ./src/server/server.js",
    "start:prod": "NODE_ENV=production nohup babel-node ./src/server/server.js > bloomburger.log 2> bloomburger.log < /dev/null &",
    "test": "npm run prettify && webpack --bail --progress --env.prod && npm run lint && npm run test:unit:coverage",
    "test:unit": "bin/run-tests.sh",
    "test:unit:coverage": "bin/run-tests-coverage.sh",
    "clean-dist": "rm -rf ./dist && mkdir dist",
    "copy-index": "cp src/index.html dist",
    "copy-assets": "cp -R src/assets dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-index && npm run copy-assets",
    "lint": "./bin/lint.sh js && ./bin/lint.sh jsx",
    "prettify": "bin/prettify.sh",
    "db-migration": "babel-node src/server/db/bin/db-migration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjwyse/bloomburger.git"
  },
  "author": "jjwyse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjwyse/bloomburger/issues"
  },
  "homepage": "https://github.com/jjwyse/bloomburger#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.3.0",
    "material-ui": "^0.20.0",
    "pg": "^6.1.4",
    "prop-types": "^15.6.1",
    "ramda": "^0.23.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-imageloader": "^2.1.0",
    "react-markdown": "^2.4.2",
    "react-redux": "^5.0.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.0.4",
    "redux-thunk": "^2.1.0",
    "styled-components": "^3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-resolver": "^1.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.2",
    "coveralls": "^2.11.6",
    "css-loader": "^0.26.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "glob": "^7.1.1",
    "helmet": "^3.4.0",
    "html-webpack-plugin": "^2.26.0",
    "if-env": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "knex": "^0.14.4",
    "mocha": "^3.0.2",
    "mockery": "^2.0.0",
    "nock": "^9.0.2",
    "node-sass": "^4.2.0",
    "opt-cli": "^1.5.1",
    "postcss-loader": "^1.2.2",
    "postcss-smart-import": "^0.6.7",
    "precss": "^1.4.0",
    "prettier": "^0.14.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-lint": "^1.8.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^1.1.0",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
